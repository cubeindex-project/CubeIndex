# Supabase project URL for API connections
PUBLIC_SUPABASE_URL=

# Supabase anonymous key for public API access
PUBLIC_SUPABASE_ANON_KEY=

# Deployment channel label for UI cues
PUBLIC_DEPLOYMENT_CHANNEL=production

# Base URL for the beta deployment (enables automatic opt-in redirects)
PUBLIC_BETA_APP_URL=https://beta.thecubeindex.com

# Base URL for the production deployment (used when linking from the beta app)
PUBLIC_PRODUCTION_APP_URL=https://thecubeindex.com

# Pino log level for server-side logging
LOG_LEVEL=info
