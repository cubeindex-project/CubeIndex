# Supabase project URL for API connections (e.g. https://xyz.supabase.co)
PUBLIC_SUPABASE_URL=

# Supabase anonymous key used by the client to authenticate requests
PUBLIC_SUPABASE_ANON_KEY=

# Pino log level for server-side logging (debug, info, warn, error)
LOG_LEVEL=info

# Cloudflare Turnstile secret key used on the server to verify tokens
TURNSTILE_SECRET_KEY=

# Cloudflare Turnstile site key exposed to the client for the widget
PUBLIC_TURNSTILE_SITE_KEY=